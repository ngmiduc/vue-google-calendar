{
  "name": "vue-google-calendar",
  "version": "0.1.0",
  "description": "vuejs google week calendar",
  "author": "Nguyen Minh Duc",
  "license": null,
  "main": "dist/vue-google-calendar.umd.js",
  "module": "dist/vue-google-calendar.esm.js",
  "unpkg": "dist/vue-google-calendar.min.js",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-google-calendar.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-google-calendar.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-google-calendar.min.js"
  },
  "dependencies": {
    "core-js": "^2.6.11",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1"
  }
}
